{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-classes-per-file":0,
        "jest/expect-expect": 0,
        "no-underscore-dangle": 0,
        "camelcase": 0
    }
}
